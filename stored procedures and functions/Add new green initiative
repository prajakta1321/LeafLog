# Stored Procedure â€” Add new green initiative
DELIMITER //
CREATE PROCEDURE AddGreenInitiative(
    IN h_id INT,
    IN init_name VARCHAR(100),
    IN start_dt DATE
)
BEGIN
    INSERT INTO GreenInitiatives (household_id, initiative_name, start_date)
    VALUES (h_id, init_name, start_dt);
END //
DELIMITER ;

# Usage:
CALL AddGreenInitiative(3, 'Community Composting', '2024-04-10');
